# ============================================================
# SCRAPER-PRO : MODE 1 - SCRAPING MASSIF (AVEC PROXIES)
# ============================================================
# Configuration complète avec proxies rotatifs
# Budget : ~280€/mois | Capacité : 10K-20K contacts/mois
# Sources : Google, LinkedIn, Facebook, Instagram, YouTube + URLs
# ============================================================

# ====================
# MODE
# ====================
SCRAPER_MODE=advanced
ENABLE_PROXIES=true

# ====================
# PostgreSQL
# ====================
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=scraper_db
POSTGRES_USER=scraper_admin
POSTGRES_PASSWORD=CHANGE_ME_STRONG_PASSWORD_HERE_Min16Chars!

# ====================
# Redis
# ====================
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=CHANGE_ME_REDIS_PASSWORD_Min12Chars!
REDIS_DB=0

# ====================
# PROXIES (MODE 1 UNIQUEMENT)
# ====================

# Oxylabs (datacenter proxies)
OXYLABS_USER=your_username_here
OXYLABS_PASS=your_password_here
OXYLABS_ENDPOINT=pr.oxylabs.io:7777

# SmartProxy (residential proxies)
SMARTPROXY_USER=your_username_here
SMARTPROXY_PASS=your_password_here
SMARTPROXY_ENDPOINT=gate.smartproxy.com:7000

# BrightData (residential proxies - optionnel)
BRIGHTDATA_USER=your_username_here
BRIGHTDATA_PASS=your_password_here
BRIGHTDATA_ENDPOINT=brd.superproxy.io:22225

# ====================
# MailWizz APIs
# ====================

# SOS-Expat MailWizz
MAILWIZZ_SOS_EXPAT_API_URL=https://mail.sos-expat.com/api
MAILWIZZ_SOS_EXPAT_API_KEY=your_sos_expat_mailwizz_api_key_here

# Ulixai MailWizz
MAILWIZZ_ULIXAI_API_URL=https://mail.ulixai.com/api
MAILWIZZ_ULIXAI_API_KEY=your_ulixai_mailwizz_api_key_here

# ====================
# SerpAPI (pour Google Search)
# ====================
SERPAPI_KEY=your_serpapi_key_here

# ====================
# Backlink Engine (optionnel)
# ====================
BACKLINK_ENGINE_URL=https://backlinks.life-expat.com/api
BACKLINK_ENGINE_API_KEY=your_backlink_api_key

# ====================
# API Security
# ====================
API_HMAC_SECRET=CHANGE_ME_RANDOM_SECRET_KEY_Min32Chars_Use_OpenSSL_Rand_Hex!

# ====================
# Dashboard
# ====================
DASHBOARD_PASSWORD=CHANGE_ME_DASHBOARD_ADMIN_PASSWORD!

# ====================
# Email Warmup
# ====================
# Daily quota limits for new domains (warmup protection)
WARMUP_ENABLED=true
WARMUP_DAILY_QUOTA_DAY1=50
WARMUP_DAILY_QUOTA_DAY7=200
WARMUP_DAILY_QUOTA_DAY14=500
WARMUP_DAILY_QUOTA_DAY30=1000

# ====================
# Logging
# ====================
LOG_LEVEL=INFO
ENVIRONMENT=production

# ====================
# CAPTCHA Service (optionnel)
# ====================
# Pour sites avec CAPTCHA (Google, LinkedIn)
CAPTCHA_SERVICE=2captcha
CAPTCHA_API_KEY=your_2captcha_api_key_here
